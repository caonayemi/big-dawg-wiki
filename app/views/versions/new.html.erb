<div class="row">
	<div class="col s10 offset-s1">
		<h1 class="brown-text text-darken-2">New Version For <%= @version.article.title %></h1>
	</div>
</div>

<div class="row">
	<%= form_for @version, html: {class: "col s8 offset-s2"} do |f| %>
		<%= f.label :content %>
		<%= f.text_area :content %>
		<%= f.label :footnotes %>
		<%= f.text_area :footnotes%>
		<button class="btn waves-effect green darken-1 waves-light" type="submit" name="action">Create New Version
		   <i class="material-icons">send</i>
		 </button>
	<% end %>
</div>
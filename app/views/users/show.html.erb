<h2>Here's Your Profile</h2>


<h4> Unpublished Versions. </h4>
<% unpublished = current_user.versions.select do |version| %>
  <% !version.is_published %>
<% end %>

<% unpublished.each do |version| %>
  <div><%= link_to version.article.title, show_version_path(version.article.slug , version) %></div>
<% end %>






<ul>
  <% if current_user %>
  <li><%= link_to 'New Article', new_article_path %></li>
  <li><%= link_to 'Logout', :logout, method: :delete %></li>
  <li>
    <form id='search-bar' action="/articles/search" method='get'>
      <input type='text' name='term'>
      <input type='submit' value='search'>
    </form>
  </li>
  <% end %>
</ul>

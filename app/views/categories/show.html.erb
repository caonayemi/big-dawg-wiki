<div class="row">
	<div class="col s8 offset-s2">
		<h1>Articles by Category: <%= @category.name %></h1>
	</div>
</div>

<% @category.versions.each do |version| %>
<div class="divider"></div>
	<div class="section deep-purple darken-2">
		<h4><%= link_to version.article.title, show_recent_path(version.article.slug), class: "index_li white-text" %></h4>
	</div>
<% end %>
